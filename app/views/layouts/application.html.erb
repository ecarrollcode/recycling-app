   <section class="some-padding" id="map">
        <div class="container">
            <h3>My Google Maps Demo</h3>
            <div id="map"></div>
            <script>
              function initMap() {
                var uluru = {lat: 34.099972, lng: -117.708281};
                var map = new google.maps.Map(document.getElementById('map'), {
                  zoom: 16,
                  center: uluru
                });
                var marker = new google.maps.Marker({
                  position: uluru,
                  map: map
                });
              }
            </script>
            <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD4piiBT9lv_CdqgsWiFBrPASbUyq2XZMI&callback=initMap"></script>
